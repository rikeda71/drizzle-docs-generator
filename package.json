{
  "name": "drizzle-docs-generator",
  "version": "0.0.1",
  "description": "A CLI tool that generates DBML files from Drizzle ORM schema definitions",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "drizzle-docs": "dist/cli/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "test:run": "vitest run",
    "lint": "oxlint",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "drizzle",
    "drizzle-orm",
    "dbml",
    "database",
    "schema",
    "documentation",
    "cli"
  ],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.24.0",
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "commander": "^13.1.0",
    "drizzle-orm": "^0.44.2"
  },
  "devDependencies": {
    "@types/node": "^22.15.21",
    "oxlint": "^0.16.10",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3",
    "vitest": "^3.1.3"
  }
}
