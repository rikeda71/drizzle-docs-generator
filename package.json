{
  "name": "drizzle-docs-generator",
  "version": "0.0.1",
  "description": "A CLI tool that generates DBML files from Drizzle ORM schema definitions",
  "keywords": [
    "cli",
    "database",
    "dbml",
    "documentation",
    "drizzle",
    "drizzle-orm",
    "schema"
  ],
  "license": "ISC",
  "author": "",
  "bin": {
    "drizzle-docs": "dist/cli/index.js"
  },
  "files": [
    "dist"
  ],
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch",
    "test": "vitest",
    "test:run": "vitest run",
    "lint": "oxlint",
    "format": "oxfmt --write",
    "format:check": "oxfmt --check",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "commander": "^13.1.0",
    "drizzle-orm": "^0.44.2"
  },
  "devDependencies": {
    "@types/node": "^22.15.21",
    "oxfmt": "^0.23.0",
    "oxlint": "^0.16.10",
    "typescript": "^5.8.3",
    "vite": "^7.3.1",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "pnpm@10.24.0"
}
